  <children>
    <Label layoutX="52.0" layoutY="36.0" text="Username:" />
    <TextField fx:id="usernameField" layoutX="122.0" layoutY="32.0" />
    <Label layoutX="52.0" layoutY="80.0" text="Password:" />
    <PasswordField fx:id="passwordField" layoutX="122.0" layoutY="76.0" />
    <Button layoutX="122.0" layoutY="123.0" mnemonicParsing="false" onAction="#login" text="Login" />
    <Button layoutX="195.0" layoutY="123.0" mnemonicParsing="false" onAction="#register" text="Register" />
    <Label fx:id="messageLabel" layoutX="39.0" layoutY="169.0" prefHeight="17.0" prefWidth="222.0" />
  </children>
